<h1>@ViewBag.title</h1>

<h3> @ViewBag.jobs.Count Result(s) </h3>

@foreach (Dictionary<string, string> job in ViewBag.jobs)
{
    <table class="job-listing">


        @foreach (KeyValuePair<string, string> pair in job)
        {
            <tdbody>
                <tr>

                    <td><b>@(pair.Key + ": ") </b> </td>
                    <td> @(pair.Value + "\n" ) </td>

                </tr>
            </tdbody>

        }


    </table>
}
}